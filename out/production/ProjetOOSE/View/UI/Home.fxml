<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Controller.HomeController">

   <children>
      <Label fx:id="labelUser" layoutX="291.0" layoutY="168.0" text="Label" textAlignment="CENTER" />
      <ButtonBar layoutX="124.0" layoutY="346.0" prefHeight="40.0" prefWidth="350.0">
        <buttons>
          <Button fx:id="glidersBtn" mnemonicParsing="false" onAction="#getGliders" text="Gliders" />
            <Button fx:id="myProfileBtn" mnemonicParsing="false" onAction="#getMyProfile" text="My Profile" />
            <Button fx:id="logoutBtn" mnemonicParsing="false" onAction="#logout" text="Log out" />
            <Button fx:id="viewBookingButton" mnemonicParsing="false" onAction="#getBooking" text="Booking" />
            <Button fx:id="addUserButton" mnemonicParsing="false" onAction="#addUser" text="addUser" />
            <Button fx:id="equipmentsButton" mnemonicParsing="false" onAction="#getEquipments" text="Equipments" />
            <Button fx:id="myCompanyBtn" layoutX="63.0" layoutY="354.0" mnemonicParsing="false" onAction="#getCompany" text="my Company" />
        </buttons>
      </ButtonBar>

   </children>
</AnchorPane>
