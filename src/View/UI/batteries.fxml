<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Controller.BatteriesUIController">

    <children>
        <ScrollPane layoutY="71.0" prefHeight="257.0" prefWidth="600.0" AnchorPane.topAnchor="71.0">
            <content>
                <GridPane fx:id="gridBatteries" gridLinesVisible="true" prefHeight="71.0" prefWidth="578.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="60.0" prefHeight="60.0" vgrow="NEVER" />
                    </rowConstraints>
                    <children>
                        <Label text="ID" GridPane.halignment="CENTER" />
                        <Label text="Wording" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                        <Label text="Power" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
                        <Label text="Refill Date" GridPane.columnIndex="3" GridPane.halignment="CENTER" />
                    </children>
                </GridPane>
            </content>
        </ScrollPane>
        <Button layoutX="267.0" layoutY="348.0" mnemonicParsing="false" onAction="#createBattery" text="New" />
      <Button layoutX="50.0" layoutY="348.0" mnemonicParsing="false" onAction="#back" text="Back" />
    </children>

</AnchorPane>
